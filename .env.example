# MIRCrew Indexer Debug - Environment Variables
# Copia questo file in .env e configura le variabili

# Credenziali MIRCrew (OBBLIGATORIO)
MIRCREW_USERNAME=your_username
MIRCREW_PASSWORD=your_password

# URL base del sito (default: https://mircrew-releases.org)
MIRCREW_BASE_URL=https://mircrew-releases.org

# Livello di debug (DEBUG, INFO, WARNING, ERROR)
DEBUG_LEVEL=INFO

# Abilita test con Selenium (true/false)
USE_SELENIUM=false
